    "lodash-es": "^4.17.21",
    "typescript": "^4.5.3",
    "vitest": "^0.34.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@metaplex-foundation/umi-options": "^0.8.5",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "dayjs": "^1.11.7"
  }
